#!/bin/sh

#exec 1>&2

DIR=$(dirname "$0")

VER_FILE=${1:-"_version.py"}

. $DIR/ver_incl.sh

echo "# autogenerated. do not edit " > $VER_FILE
echo "GIT_DATE = \"$DATE\"" >> $VER_FILE
echo "GIT_HASH = \"$HASH\"" >> $VER_FILE
echo "GIT_TAG = \"$TAG\"">> $VER_FILE
echo "BUILD_DATE = \"`date -Iseconds --utc`\"" >> $VER_FILE

